{
  "name": "cloud-stove-ui",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "build": "rimraf dist && webpack --bail --color",
    "start": "webpack-dev-server --hot --inline --progress --port 1232",
    "production": "node server/server.js",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "e2e": "protractor",
    "webdriver-update": "webdriver-manager update",
    "postinstall": "npm run webdriver-update && typings install && npm run build",
    "mockdb": "json-server --watch docs/db.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
