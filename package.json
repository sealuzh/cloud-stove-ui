    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.0",
    "typedoc": "~0.5.5",
    "typedoc-plugin-external-module-name": "dschoeni/typedoc-plugin-external-module-name#caaaa0981342def876f923e92d1fd020d0a24840",
    "typedoc-plugin-ui-router": "1.0.6",
